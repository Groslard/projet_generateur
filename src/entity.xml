<?xml version="1.0" encoding="UTF-8"?>
<root>
	<collections>
		<listdef id="list-String" min="10" max="20" type="String"/>
		<listdef id="list-Coucou"  type="Coucou"/>
		<listdef id="list-list-Coucou" type="list-Coucou"/>
		<setdef id="set-String" type="String"/>
	</collections>
	
	<entity id="Balise" parent="list-list-Coucou">		
		<attribute name="monString" type="String" />
		<attribute name="mesCoucou" type="list-list-Coucou"/>
		<attribute name="noms"  type="list-String"/>
		
	</entity>
	
	<entity id="Coucou" >
		<attribute name="Nom" type="String"/>
		<attribute name="Numero" type="int"/>
		<attribute name="mesStrings" type="set-String"/>
	</entity>
</root>